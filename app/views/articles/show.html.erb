<div class= "container"><h1><%= @article.title%></h1>
  <p><%= @article.content %></p>
  <%= link_to "Back", articles_path %>
  <%= link_to "Edit Article", edit_article_path %>
  <%= link_to article_path(@article), method: :delete,
    data: { confirm: "Are you sure?" } do %>
      <i class="fas fa-trash-alt"></i>
  <% end %></div>
